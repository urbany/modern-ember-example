{
  "extends": "@ember/app-tsconfig",
  "include": ["app", "tests", "types"],
  "compilerOptions": {
    "plugins": [
      {
        "name": "@glint/tsserver-plugin"
      }
    ],
    "allowJs": true,
    "paths": {
      "modern-ember-example/tests/*": ["./tests/*"],
      "modern-ember-example/*": ["./app/*"],
      "*": ["./types/*"]
    },
    "types": [
      "ember-source/types",
      "@embroider/core/virtual",
      "vite/client",
      "@glint/ember-tsc/types",
      "unplugin-icons/types/raw"
    ]
  }
}
