{
  "name": "modern-ember-example",
  "version": "0.0.0",
  "private": true,
  "description": "A modern example Ember app using Warp Drive and TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/urbany/modern-ember-example.git"
  },
  "homepage": "https://urbany.github.io/modern-ember-example/",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc",
    "start": "vite",
    "test": "vite build --mode development && testem ci"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "dependencies": {
    "@ember-data/debug": "5.8.0",
    "@warp-drive/build-config": "5.8.0",
    "@warp-drive/core": "5.8.0",
    "@warp-drive/ember": "5.8.0",
    "@warp-drive/json-api": "5.8.0",
    "@warp-drive/legacy": "5.8.0",
    "@warp-drive/utilities": "5.8.0",
    "daisyui": "^5.5.5",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@ember/app-tsconfig": "^1.0.3",
    "@ember/optional-features": "^2.3.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/compat": "^4.1.11",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.2.9",
    "@embroider/legacy-inspector-support": "^0.1.3",
    "@embroider/macros": "^1.19.5",
    "@embroider/router": "^3.0.6",
    "@embroider/vite": "^1.4.3",
    "@eslint/js": "^9.39.1",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "^1.5.2",
    "@glint/ember-tsc": "^1.0.8",
    "@glint/template": "^1.7.3",
    "@glint/tsserver-plugin": "^2.0.8",
    "@iconify-json/lucide": "^1.2.76",
    "@rollup/plugin-babel": "^6.1.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/qunit": "^2.19.13",
    "@types/rsvp": "^4.0.9",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.0",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "^6.8.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-deprecation-workflow": "^3.4.0",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-simple-auth": "^8.1.0",
    "ember-simple-auth-token": "^6.0.1",
    "ember-source": "^6.9.0",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.5",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-qunit": "^8.2.5",
    "globals": "^16.5.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.1",
    "prettier-plugin-ember-template-tag": "^2.1.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "qunit": "^2.24.2",
    "qunit-dom": "^3.5.0",
    "simple-git-hooks": "^2.13.1",
    "stylelint": "^16.26.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^4.1.17",
    "testem": "^3.16.0",
    "tracked-built-ins": "^4.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "unplugin-icons": "^22.5.0",
    "vite": "^7.2.4"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm dlx lint-staged"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix --quiet --report-unused-disable-directives",
      "prettier --write"
    ],
    "**/*.{gjs,gts}": [
      "eslint --fix --quiet --report-unused-disable-directives",
      "prettier --write"
    ],
    "**/*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "**/*.md": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
