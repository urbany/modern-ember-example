{
  "name": "modern-ember-example",
  "version": "0.0.0",
  "private": true,
  "description": "A modern example Ember app using Warp Drive and TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/urbany/modern-ember-example.git"
  },
  "homepage": "https://urbany.github.io/modern-ember-example/",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc",
    "start": "vite",
    "test": "vite build --mode development && ember test --path dist"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "dependencies": {
    "@ember-data/debug": "5.8.1",
    "@warp-drive/build-config": "5.8.1",
    "@warp-drive/core": "5.8.1",
    "@warp-drive/ember": "5.8.1",
    "@warp-drive/json-api": "5.8.1",
    "@warp-drive/legacy": "5.8.1",
    "@warp-drive/utilities": "5.8.1",
    "daisyui": "^5.5.14",
    "ember-primitives": "^0.53.0",
    "ember-truth-helpers": "^5.0.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/eslint-parser": "^7.28.6",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.6",
    "@babel/runtime": "^7.28.6",
    "@ember/app-tsconfig": "^2.0.0",
    "@ember/optional-features": "^2.3.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/compat": "^4.1.12",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.4.2",
    "@embroider/legacy-inspector-support": "^0.1.3",
    "@embroider/macros": "^1.19.6",
    "@embroider/router": "^3.0.6",
    "@embroider/vite": "^1.5.0",
    "@eslint/js": "^9.39.2",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "^1.5.2",
    "@glint/ember-tsc": "^1.0.9",
    "@glint/template": "^1.7.3",
    "@glint/tsserver-plugin": "^2.0.9",
    "@iconify-json/lucide": "^1.2.85",
    "@rollup/plugin-babel": "^6.1.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/qunit": "^2.19.13",
    "@types/rsvp": "^4.0.9",
    "babel-plugin-ember-template-compilation": "^4.0.0",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.1",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~6.9.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-deprecation-workflow": "^4.0.0",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-simple-auth": "^8.2.0",
    "ember-simple-auth-token": "^6.0.1",
    "ember-source": "^6.10.0",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.5",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-qunit": "^8.2.5",
    "globals": "^17.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "qunit": "^2.25.0",
    "qunit-dom": "^3.5.0",
    "simple-git-hooks": "^2.13.1",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-tailwindcss": "^1.0.1",
    "tailwindcss": "^4.1.18",
    "testem": "^3.17.0",
    "tracked-built-ins": "^4.1.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "unplugin-icons": "^23.0.1",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": ">= 20"
  },
  "ember": {
    "edition": "octane"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm dlx lint-staged"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix --quiet --report-unused-disable-directives",
      "prettier --write"
    ],
    "**/*.{gjs,gts}": [
      "eslint --fix --quiet --report-unused-disable-directives",
      "prettier --write"
    ],
    "**/*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "**/*.md": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
